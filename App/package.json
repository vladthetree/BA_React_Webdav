{
  "name": "pwa_message_receiver",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "A progressive web app for receiving mp4 messages",
  "keywords": [
    "pwa",
    "webdav",
    "react",
    "webpack"
  ],
  "scripts": {
    "start:all": "npm-run-all --parallel build:pwa serve:prox serve:pwa",
    "start:pwa": "npm-run-all --parallel build:pwa serve:pwa",
    "start:proxyServer": "npm-run-all --parallel serve:proxyServer",
    "watch:pwa": "npm-run-all --parallel watch:build:pwa serve:pwa",
    "watch:all": "npm-run-all --parallel watch:build:pwa serve:pwa ",
    "serve:proxyServer": "node ./server/proxy.mjs",
    "serve:pwa": "node ./server/pwa.mjs",
    "build:pwa": "webpack --config pwa/webpack.config.cjs",
    "watch:build:pwa": "webpack --config pwa/webpack.config.cjs --watch",
    "format": "prettier --write 'pwa/src/**/*.{js,jsx,css}' 'server/*.mjs'"
  },
  "dependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "buffer": "^6.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-http-proxy": "^1.6.3",
    "http-proxy-middleware": "^2.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.3.4",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.8.1",
    "react-use": "^17.4.0",
    "webdav": "^5.0.0-r1",
    "workbox-background-sync": "^6.5.4",
    "workbox-broadcast-update": "^6.5.4",
    "workbox-cacheable-response": "^6.5.4",
    "workbox-core": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-navigation-preload": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-range-requests": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4",
    "workbox-streams": "^6.5.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@use-gesture/react": "^10.2.25",
    "babel-loader": "^9.1.2",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^11.0.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "react-scripts": "^5.0.1",
    "webpack": "^5.77.0",
    "webpack-cli": "^5.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
